{
  "name": "hunthelper-browser-extension",
  "private": true,
  "version": "0.0.1",
  "description": "Browser extension for quickly adding web sites to HuntHelper",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp --production --vendor=chrome && gulp pack --vendor=chrome",
    "build:firefox": "gulp --production --vendor=firefox && gulp pack --vendor=firefox",
    "build:opera": "gulp --production --vendor=opera && gulp pack --vendor=opera",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera"
  },
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-cli": "6.x.x",
    "babel-core": "^6.22.1",
    "babel-eslint": "7.x.x",
    "babel-loader": "7.x.x",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "6.x.x",
    "bulma": "^0.5.0",
    "chai": "4.x.x",
    "chromereload": "0.x.x",
    "css-loader": "^0.28.4",
    "debounce": "1.x.x",
    "del": "3.x.x",
    "eslint": "4.x.x",
    "eslint-loader": "1.x.x",
    "file-loader": "^0.11.2",
    "gulp": "3.x.x",
    "gulp-bump": "2.x.x",
    "gulp-cache": "0.x.x",
    "gulp-clean-css": "3.x.x",
    "gulp-filter": "5.x.x",
    "gulp-git": "2.x.x",
    "gulp-if": "2.x.x",
    "gulp-imagemin": "3.x.x",
    "gulp-json-transform": "0.x.x",
    "gulp-less": "3.x.x",
    "gulp-livereload": "3.x.x",
    "gulp-plumber": "1.x.x",
    "gulp-sass": "3.x.x",
    "gulp-sequence": "0.x.x",
    "gulp-sourcemaps": "2.x.x",
    "gulp-tag-version": "1.x.x",
    "gulp-util": "3.x.x",
    "gulp-vueify": "0.0.3",
    "gulp-zip": "4.x.x",
    "require-dir": "0.x.x",
    "url-loader": "^0.5.9",
    "vinyl-named": "1.x.x",
    "vue": "^2.2.1",
    "vue-loader": "12.x.x",
    "vue-template-compiler": "^2.2.1",
    "vueify-insert-css": "^1.0.0",
    "vuex": "^2.3.1",
    "vuex-persistedstate": "^2.0.0",
    "webpack": "^3.4.1",
    "webpack-stream": "3.x.x",
    "yargs": "8.x.x"
  },
  "dependencies": {
    "pug": "^2.0.0-rc.3"
  }
}
