{
  "name": "__MSG_appName__",
  "short_name": "__MSG_appShortName__",
  "description": "__MSG_appDescription__",
  "version": "0.0.3",
  "manifest_version": 2,
  "default_locale": "en",
  "__chrome__minimum_chrome_version": "33",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "__chrome__background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "__chrome__browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "__MSG_browserActionTitle__",
    "default_popup": "pages/popup.html"
  },
  "__chrome__permissions": [
    "activeTab",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "contextMenus",
    "cookies",
    "debugger",
    "declarativeContent",
    "identity",
    "management",
    "notifications",
    "pageCapture",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "https://www.hunthelper.com/*",
    "https://localhost.ssl:5000/*"
  ],
  "__chrome__web_accessible_resources": [
    "images/*.png",
    "fonts/CooperHewitt-*.woff"
